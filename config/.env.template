# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env file to version control

# Telegram API (Required)
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here
TELEGRAM_PHONE=+1234567890

# Discord (Optional - Use with caution)
DISCORD_USER_TOKEN=your_discord_token_here

# Google Gemini API (Required)
GEMINI_API_KEY=your_gemini_api_key_here

# Firebase Cloud Messaging (for Android notifications)
FCM_SERVER_KEY=your_fcm_server_key_here
FCM_DEVICE_TOKEN=your_android_device_token_here

# Alternative: Pushbullet
PUSHBULLET_TOKEN=your_pushbullet_token_here

# Optional: Custom webhook
WEBHOOK_URL=https://your-server.com/webhook
WEBHOOK_TOKEN=your_webhook_auth_token